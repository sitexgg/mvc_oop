	<script>
		jQuery(document).ready(function ($)
		{
			$('.RS form').submit(function ()
			{
				var data = $(this).serialize();
				$.post($(this).attr("action"), data, function (data, status)
				{
					if (status == 'success') {
						if ($(data).find(".RS").find("form").html() == undefined) {
							$(".RS").html($(data).find(".RS").html());
						}
					}
				});
				return false;
			});
		});
	</script>
