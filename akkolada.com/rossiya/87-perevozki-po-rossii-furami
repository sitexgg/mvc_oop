    (function($) {
        $(function() {
            var LSregion = $.cookie('region');

              $(document).click(function(event) {
                if ($(event.target).closest('#allregions').length) return;
                $('#allregions').slideUp();
                event.stopPropagation();
              });

            $('#region').live('click', function(){
                $('#allregions').slideDown();
                return false;
            });
            $('#allregions a').live('click', function(){
                $.cookie('region', $(this).text(), {expires: 7, path: '/'});
                $('#region').text($(this).text());
                $('#allregions').slideUp();
                location.reload();
                return false;
            });

            if(LSregion && LSregion !== 'undefined'){
                $('#region').text(LSregion);
            } else {
                window.onload = function () {
                    ymaps.ready(function(){
                        ymaps.geolocation.get({
                            provider: 'yandex',
                            autoReverseGeocode: true
                        }).then(function (result) {

                            var resGeoCoder = result.geoObjects.get(0).properties.get('text');

                            if(resGeoCoder === 'Россия, Москва'){
                                resGeoCoder = 'Москва и Московская область';
                            }

                            $('#region').text(resGeoCoder);
                            $.cookie('region', resGeoCoder, { expires: 7, path: '/' });
                        });
                    });

                }
            }

            // Обрабатываем массив с условиями
			function calc(){
				LSregion = $.cookie('region');
				var arr = '[["0","\u041c\u043e\u0441\u043a\u0432\u0430 \u0438 \u041c\u043e\u0441\u043a\u043e\u0432\u0441\u043a\u0430\u044f \u043e\u0431\u043b\u0430\u0441\u0442\u044c","#h-tel-new, .form_feedback .formBody strong, footer .phone strong","\u003Ca id=\\\"head_tel\\\" href=\\\"tel:8 800 707-08-81\\\"\u003E8 800 707-08-81\u003C\/a\u003E\u003Cp\u003E\u0417\u0432\u043e\u043d\u043e\u043a \u043f\u043e \u0420\u043e\u0441\u0441\u0438\u0438 \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0439\u003C\/p\u003E"],["1","\u041c\u043e\u0441\u043a\u0432\u0430 \u0438 \u041c\u043e\u0441\u043a\u043e\u0432\u0441\u043a\u0430\u044f \u043e\u0431\u043b\u0430\u0441\u0442\u044c","#result .RS #h-tel-new","\u003Ca id=\\\"head_tel\\\" href=\\\"tel:8 800 707-08-81\\\"\u003E8 800 707-08-81\u003C\/a\u003E\u003Cp\u003E\u0417\u0432\u043e\u043d\u043e\u043a \u043f\u043e \u0420\u043e\u0441\u0441\u0438\u0438 \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0439\u003C\/p\u003E"]]';
				$.each(JSON.parse(arr), function(i, obj) {
					if(obj[0]=='1'){
						var condition = 'LSregion == obj[1]';
					} else {
						var condition = 'LSregion != obj[1]';
					}
					if(eval(condition) && LSregion){
						$(obj[2]).html(obj[3]);
						return false;
					}
				});
			}
			calc();

			// Если первый заход - перезапускаем функцию для обновления данных.
			if(!LSregion){
				var intervalID = setInterval(function(){
				  calc();
				  if($('#region').text()!='Определяем...'){
					  clearInterval(intervalID);
				  }
				}, 1000);
			}


        });
    })(jQuery);

