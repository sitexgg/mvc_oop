<script async>
	jQuery(function ()
	{
		/* выбор города */
		jQuery('.delivery_list').click(function ()
		{
			jQuery(".cities_list").slideToggle('fast');
		});
		jQuery('ul.cities_list li').click(function ()
		{
			var tx = jQuery(this).html();
			/*alert (tx);*/
			var tv = jQuery(this).attr('href');
			/*alert (tv);*/
			jQuery(".cities_list").slideUp('fast');
			jQuery(".delivery_list span").html(tx);
			window.location.href = tv;
			jQuery(".delivery_text").html(tv);
		});
	})
</script>

